type Query{
    actors: [Actor]
}

type Movie @key(fields: "id") @extends {
    id: ID @external
    actor: Actor
}

type Actor {
    actorId: ID!
    name: String
    hasWonAnOscar: Boolean
    id: [ID]
}

scalar _FieldSet

directive @key(fields: _FieldSet!) on OBJECT | INTERFACE
directive @extends on OBJECT | INTERFACE
directive @external on FIELD_DEFINITION
